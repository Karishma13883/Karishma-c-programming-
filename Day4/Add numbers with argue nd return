#include <stdio.h>

int add_numbers(int num1, int num2) {
    int sum = num1 + num2;
    return sum;
}

int main() {
    int a, b, result;
    
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    
    result = add_numbers(a, b);
    
    printf("Sum of %d and %d is %d\n", a, b, result);
    
    return 0;
}
